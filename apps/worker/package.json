{
  "name": "@polymarket/worker",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc || echo 'Build completed with errors, but files generated'",
    "dev": "tsx watch src/index.ts",
    "start": "pnpm build && node --max-old-space-size=2048 dist/index.js",
    "start:prod": "node --max-old-space-size=2048 dist/index.js",
    "clean": "rm -rf dist",
    "populate": "tsx src/fetch-all-traders.ts && tsx src/update-trader-locations.ts && tsx src/test-discovery.ts",
    "geo": "tsx src/add-geolocation.ts",
    "sync-map": "tsx src/trigger-map-sync.ts",
    "fix-twitter": "tsx src/fix-twitter-traders.ts",
    "clean-fallback": "tsx src/clean-fallback-profiles.ts",
    "run-sync-public": "tsx src/run-sync-public.ts",
    "fix-scores": "tsx src/recalculate-scores.ts"
  },
  "dependencies": {
    "@polymarket/database": "workspace:*",
    "@polymarket/shared": "workspace:*",
    "bullmq": "^5.1.5",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "viem": "^2.21.54"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

